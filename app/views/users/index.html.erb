<p id="notice"><%= notice %></p>

<h1><%= User.model_name.human %></h1>

<table>
  <thead>
  <tr>
    <th><%= User.human_attribute_name(:email) %></th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= link_to t('views.common.show'), user %></td>
      <% if user_signed_in? && user.id == current_user.id %>
        <td><%= link_to t('views.common.edit'), edit_user_registration_path(user) %></td>
        <!--      <td><%#= link_to t('views.common.destroy'), user, method: :delete, data: { confirm: t('views.common.delete_confirm') } if user.id == current_user.id %></td>-->
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @users %>

<br>